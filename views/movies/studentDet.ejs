<% include ../partials/header %>

<div class="register">

	<h1 class=" d-flex justify-content-center" style="color: white; padding: 9px;">
		Details of <%=data.sname%>
	</h1>
	<div class=row>
		<form action="/MyAcc" class="mx-auto" method="POST">
			<fieldset disabled="disabled">
				<div class="form-group">
					<input class="form-control " type="text" name="sName" placeholder="Name" value="Name :> <%=data.sname%>" required>
				</div>
				<div class="form-group">
					<input class="form-control " type="text" name="sUsn" placeholder="USN" value="USN:><%=data.usn%>"required>
				</div>
				<div class="form-group">
					<input class="form-control " type="text" name="sDepartment" placeholder="Department" value="Branch:> <%=data.department%>"required>
				</div>
				
				<div class="form-group">
					<input class="form-control " type="text" name="sAddress" placeholder="Address" value="Address:> <%=data.address%>"required>
				</div>
				<div class="form-group">
					<input class="form-control " type="text" name="sContactNo" placeholder="Contact No" value="contach No:><%=data.contactNo%>"required>
				</div>
				<div class="form-group">
					<input class="form-control " type="text" name="sCGPA" placeholder="CGPA" value="CGPA:> <%=data.cgpa%>" required>
				</div>
				<div class="form-group">
					<input class="form-control " type="text" name="resume" placeholder="Resume link" value="Resume:><%=data.resume%>" required>
				</div>
			</fieldset>
			<a href="/movies">Go Back </a>
		</form>
	</div>
	
</div>
<% include ../partials/footer %>